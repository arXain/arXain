  "source": "pragma solidity ^0.4.11;\n\nimport \"./Mortal.sol\";\n\ncontract Manuscript{\n    string corpus;\n    Mortal m;\n\n    function Manuscript(){\n        corpus = \"Give me Science!\";\n        m = new Mortal();\n    }\n    function getCorpus() constant returns (string) {\n        return corpus;\n    }\n    function setCorpus(string _corpus){\n        corpus = _corpus;\n    }\n    function getAddress() constant returns (address) {\n        return m.getOwner();\n    }\n}\n",
